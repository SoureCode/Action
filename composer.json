{
    "name": "sourecode/action",
    "type": "library",
    "description": "Library to build process pipelines",
    "keywords": [
        "process",
        "action",
        "pipeline"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Schilling",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "symfony/process": "^5.3 || ^6.0",
        "symfony/console": "^5.3 || ^6.0",
        "symfony/string": "^5.3 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Component\\Action\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
